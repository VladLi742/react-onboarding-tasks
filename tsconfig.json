{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "ESNext",
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@redux/*": ["src/redux/*"],
      "@ts/*": ["src/ts/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": ["./src/"]
}
